# Solana Trading MCP Server Environment
# Copy this file to .env and fill in your actual values

# Using a generated keypair since we don't have the actual private key
# This will be automatically generated at runtime if empty
SOLANA_PRIVATE_KEY=""

# Using Solana mainnet RPC
RPC_URL="https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY"

# xAI Configuration
XAI_API_KEY=your_xai_api_key_here
XAI_MODEL=grok-3-mini-fast
NEXT_PUBLIC_XAI_ENABLED=true

# OpenAI Configuration for Image Generation
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_IMAGE_MODEL=gpt-image-1
OPENAI_MODEL=gpt-4o-mini
NEXT_PUBLIC_OPENAI_ENABLED=true

# Phala TEE Configuration
PHALA_API_KEY=your_phala_api_key_here
PHALA_VMID=your_phala_vmid_here
PHALA_APP_ID=your_phala_app_id_here
PHALA_INSTANCE_ID=your_phala_instance_id_here
NEXT_PUBLIC_PHALA_ENABLED=true

# Google Gemini Image Generation (Backup)
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here
NEXT_PUBLIC_GEMINI_ENABLED=true

# RedPill AI Configuration (TEE-secured DeepSeek)
REDPILL_API_KEY=your_redpill_api_key_here
REDPILL_MODEL=phala/deepseek-r1-70b
NEXT_PUBLIC_REDPILL_ENABLED=true

# Solana & Metaplex Configuration
NEXT_PUBLIC_SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
METAPLEX_SOLANA_DAS_RPC_URL=https://mainnet-aura.metaplex.com/YOUR_METAPLEX_KEY
NEXT_PUBLIC_SOLANA_NETWORK=mainnet-beta
TREASURY_WALLET=your_treasury_wallet_address

# Production fallbacks
NEXT_PUBLIC_MCP_ENABLED=true
NODE_ENV=production

# Server port
PORT=3000